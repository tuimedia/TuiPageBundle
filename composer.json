{
    "name": "tuimedia/page-bundle",
    "description": "An API for managing rich, versioned, multilingual content.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Robinson",
            "email": "matt@tuimedia.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tui\\PageBundle\\": "src/"
        }
    },
    "require": {
      "php": "^8.1",
      "doctrine/orm": "^2.6",
      "doctrine/dbal": "^2.6|^3.0",
      "opis/json-schema": "^1.0",
      "symfony/framework-bundle": "^5.4|^6.2.6",
      "symfony/cache": "^5.4|^6.2.6",
      "symfony/http-client": "^5.4|^6.2.6",
      "symfony/http-foundation": "^5.4|^6.2.6",
      "symfony/property-access": "^5.4|^6.2.6",
      "symfony/property-info": "^5.4|^6.2.6",
      "symfony/security-core": "^5.4|^6.2.6",
      "symfony/serializer": "^5.4|^6.2.6",
      "symfony/uid": "^5.4|^6.2.6",
      "symfony/validator": "^5.4|^6.2.6",
      "voku/anti-xss": "^4.1",
      "typesense/typesense-php": "^4.7"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-doctrine": "^1.3",
        "doctrine/doctrine-bundle": "^2.9"
    }
}
